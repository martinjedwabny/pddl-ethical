(define (domain grounded-hospital-gen)
  (:requirements :typing) 
  (:predicates 
     (is-violated-p-fast)
     (is-satisfied-p-fast)
     (is-violated-p-lied)
     (is-satisfied-p-lied)
     (athouse)
     (attoll)
     (atroad)
     (athospital)
     (barrieropen)
     (ida)
     (idb)
     (athighway)
     (tookhighway)
     (lied)
  )
  (:action gotoll
   :precondition 
     (and 
      (athouse) 
     )
   :effect 
     (and 
      (attoll) 
      (not (athouse)) 
     )
  )
  (:action goroad
   :precondition 
     (and 
      (athouse) 
     )
   :effect 
     (and 
      (atroad) 
      (not (athouse)) 
     )
  )
  (:action goroadhospital
   :precondition 
     (and 
      (atroad) 
     )
   :effect 
     (and 
      (athospital) 
      (not (atroad)) 
     )
  )
  (:action gohighwayhospital
   :precondition 
     (and 
      (athighway) 
     )
   :effect 
     (and 
      (athospital) 
      (not (athighway)) 
     )
  )
  (:action presenta
   :precondition 
     (and 
      (attoll) 
     )
   :effect 
     (and 
      (barrieropen) 
      (ida) 
     )
  )
  (:action presentb
   :precondition 
     (and 
      (attoll) 
     )
   :effect 
     (and 
      (barrieropen) 
      (idb) 
      (lied) 
     )
  )
  (:action takehighway
   :precondition 
     (and 
      (attoll) 
      (barrieropen) 
     )
   :effect 
     (and 
      (athighway) 
      (tookhighway) 
      (not (attoll)) 
     )
  )
)
